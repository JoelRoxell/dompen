{
  "name": "dompen",
  "version": "0.1.0",
  "description": "Node.js boilerplace with Docker for microservices",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/nyc --show-process-tree ./node_modules/.bin/mocha --compilers js:babel-core/register './src/**/*.spec.js'",
    "test:native": "./node_modules/.bin/mocha --harmony-async-await './src/**/*.spec.js'",
    "test:watch": "./node_modules/.bin/mocha --compilers js:babel-core/register --watch './src/**/*.spec.js'",
    "precommit": "./node_modules/.bin/eslint -c .eslintrc src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zappen999/dompen.git"
  },
  "keywords": [
    "Docker",
    "Nodejs",
    "Microservice"
  ],
  "author": "Johan Kanefur <johan.canefur@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zappen999/dompen/issues"
  },
  "homepage": "https://github.com/zappen999/dompen#readme",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.7.1",
    "eslint-config-google": "^0.6.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.1",
    "husky": "^0.11.9",
    "istanbul": "^0.4.5",
    "nyc": "^8.3.1"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.1.2"
  }
}
